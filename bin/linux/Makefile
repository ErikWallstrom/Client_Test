build:
	g++ ../../src/Main.cpp ../../src/Connection.cpp ../../src/Scene.cpp ../../src/Window.cpp  ../../src/Game_Object.cpp  ../../src/Event_Handler.cpp  ../../src/Vector2d.cpp ../../src/Texture_Handler.cpp ../../src/Core.cpp -Wall -Wextra -Wfatal-errors -ggdb3 -o Program -std=c++11 -lSDL2 -lSDL2_net -lSDL2_ttf -lSDL2_mixer -lSDL2_image -llua53
debug:
	gdb ./Program

