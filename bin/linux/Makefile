build:
	g++ ../../src/Connection.c ../../src/Main.cpp ../../src/Scene.cpp ../../src/Window.cpp  ../../src/Game_Object.cpp  ../../src/Event_Handler.cpp  ../../src/Vector2d.cpp ../../src/Texture_Handler.cpp ../../src/Core.cpp -std=c++11 -Wall -Wextra -Wfatal-errors -ggdb3 -o Program -lSDL2 -lSDL2_net -lSDL2_ttf -lSDL2_mixer -lSDL2_image -llua53
